{
	"name": "3m5/typo3-enhanced-backend",
	"type": "typo3-cms-extension",
	"description": "The Enhanced Backend Extension improves the user experience (UX) in the TYPO3 backend, provides customization options for editors and improves the look and feel of TYPO3 so that it represents a modern and intuitive eCMS.",
	"authors": [
		{
			"name": "Jan Suchandt",
			"email": "jan.suchandt@3m5.de",
			"homepage": "https://www.3m5.de",
			"role": "Developer"
		},
		{
			"name": "Steffen Thiele",
			"email": "steffen.thiele@3m5.de",
			"homepage": "https://www.3m5.de",
			"role": "Developer"
		},
		{
			"name": "Nicole Schneider",
			"email": "nicole.schneider@3m5.de",
			"homepage": "https://www.3m5.de",
			"role": "Developer"
		},
		{
			"name": "Felix Hellwig",
			"email": "nicole.schneider@3m5.de",
			"homepage": "https://www.3m5.de",
			"role": "Developer"
		}
	],
	"license": "GPL-3.0-or-later",
	"require": {
		"typo3/cms-core": "^11.0",
		"typo3/cms-setup": "^11.0",
		"typo3/cms-backend": "^11.0",
		"typo3/cms-sys-note": "^11.0"
	},
	"require-dev": {
		"typo3/testing-framework": "^6.8",
		"typo3/cms-styleguide": "^11.0"
	},
	"autoload": {
		"psr-4": {
			"DMF\\EnhancedBackend\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"DMF\\EnhancedBackend\\Tests\\": "Tests"
		}
	},
	"replace": {
		"typo3-ter/enhanced-backend": "self.version"
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin"
	},
	"scripts": {
		"post-autoload-dump": [
			"TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
		]
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/public",
			"extension-key": "enhanced-backend"
		}
	}
}
